{
  "name": "warp-template-typescript",
  "version": "0.0.1",
  "description": "Warp contracts - Typescript template",
  "author": "Redstone Team <dev@redstone.finance>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "setup-pyenv": "conda create --name rgsys --file requirements.txt && conda activate rgsys",
    "clean": "rimraf ./dist",
    "build-ts": "node build.js",
    "build": "set NODE_OPTIONS=--trace-warnings && yarn run clean && yarn run build-ts",
    "deploy": "set NODE_OPTIONS=--trace-warnings && ts-node ./scripts/deploy.ts",
    "qna": "set NODE_OPTIONS=--trace-warnings &&  ts-node ./scripts/qna.ts",
    "fund": "set NODE_OPTIONS=--trace-warnings &&  ts-node ./scripts/fund.ts",
    "setup": "set NODE_OPTIONS=--trace-warnings &&  ts-node ./scripts/setup.ts",
    "rtest": "set NODE_OPTIONS=--trace-warnings &&  ts-node ./scripts/test.ts"
  },
  "engines": {
    "node": ">=16.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "28.1.6",
    "@types/node": "18.0.6",
    "arlocal": "^1.1.42",
    "arweave": "^1.11.4",
    "copyfiles": "^2.4.0",
    "dotenv": "^16.0.3",
    "esbuild": "0.12.5",
    "jest": "^27.4.3",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.9.11",
    "bert-tokenizer": "^1.1.8",
    "onnxruntime-node": "^1.13.1",
    "pythonia": "^1.0.0",
    "warp-contracts": "^1.2.46"
  }
}
